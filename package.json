{
  "name": "chat-logger",
  "version": "0.10.6",
  "description": "A logger for Telegram messages",
  "main": "./dist/npm/index.js",
  "scripts": {
    "clean:www": "rm -rf ./dist/www",
    "clean:css": "rm -rf ./dist/css",
    "clean:dist": "rm -rf ./dist",
    "clean:db": "rm -rf ./cache",
    "clean": "npm-run-all --parallel clean:dist clean:db",
    "mkdir": "mkdir -p ./dist/www/lib/js ./dist/www/css ./cache",
    "start:telegram": "babel-node ./examples/telegram.js",
    "start:amqplib": "babel-node ./examples/amq.js",
    "start:wascally": "babel-node ./examples/wascally.js",
    "start:db": "rethinkdb --directory ./cache/rethinkdb_data",
    "start:telegram-rethinkdb": "babel-node ./examples/telegram-rethinkdb.js",
    "start:www": "http-server -p 8081 ./dist/www",
    "start:wsapi": "babel-node ./src/www/api.js",
    "start:pm2:www": "pm2 start npm --name \"webserver\" -- run start:www",
    "start:pm2:db": "pm2 start npm --name \"database\" -- run start:db",
    "start:pm2:telegram": "pm2 start npm --name \"telegram-rethinkdb\" -- run start:telegram-rethinkdb",
    "start:pm2:wsapi": "pm2 start npm --name \"wsapi\" -- run start:wsapi",
    "prestart": "pm2 delete all || true",
    "start": "npm-run-all --parallel start:pm2:*",
    "delete:pm2:db": "pm2 delete database",
    "prereset:db": "npm-run-all clean:db mkdir delete:pm2:db start:pm2:db",
    "reset:db": "babel-node ./src/db/setupRethinkdb.js",
    "test": "ava --require 'babel-core/register'",
    "build:npm:main": "rollup -i ./src/lib/index.js -o ./dist/npm/index.js --config ./config/rollup.config_cjs.js",
    "build:npm:debug": "rollup -i ./src/lib/debug.js -o ./dist/npm/debug.js --config ./config/rollup.config_cjs.js",
    "build:npm:helpers:amq": "rollup -i ./src/lib/amqHelpers.js -o ./dist/npm/amqHelpers.js --config ./config/rollup.config_cjs.js",
    "build:npm:helpers:wascally": "rollup -i ./src/lib/wascallyHelpers.js -o ./dist/npm/wascallyHelpers.js --config ./config/rollup.config_cjs.js",
    "build:npm:helpers:telegram": "rollup -i ./src/lib/telegramHelpers.js -o ./dist/npm/telegramHelpers.js --config ./config/rollup.config_cjs.js",
    "build:npm:helpers:rethinkdb": "rollup -i ./src/lib/rethinkdbHelpers.js -o ./dist/npm/rethinkdbHelpers.js --config ./config/rollup.config_cjs.js",
    "build:npm": "npm-run-all --parallel build:npm:**",
    "build:js:vendors:babelhelpers": "babel-external-helpers -t umd > ./dist/www/lib/js/babel-helpers.js",
    "build:js:vendors:react": "cp ./node_modules/react/dist/react.js ./dist/www/lib/js/.",
    "build:js:vendors:react-dom": "cp ./node_modules/react-dom/dist/react-dom.js ./dist/www/lib/js/.",
    "build:js:vendors:react-redux": "cp ./node_modules/react-redux/dist/react-redux.js ./dist/www/lib/js/.",
    "build:js:vendors:redux": "cp ./node_modules/redux/dist/redux.js ./dist/www/lib/js/.",
    "build:js:vendors:lodash-fp": "rollup -n LodashFP -i ./src/vendors/lodash.js -o ./dist/www/lib/js/lodash-custom.js --config config/rollup_config_vendors.js",
    "build:js:vendors:crypto": "browserify -s Crypto -r crypto -o ./dist/www/lib/js/crypto.js",
    "build:js:vendors:uuid": "rollup -n uuid -i ./src/vendors/uuid.js -o ./dist/www/lib/js/uuid.js --config config/rollup_config_vendors.js",
    "_failed_build:js:vendors:rethinkdb-websocket-client": "rollup -n RethinkdbWebsocketClient -i ./src/vendors/rethinkdb.js -o ./dist/www/lib/js/rethinkdb-websocket-client.js --config config/rollup_config_vendors.js",
    "build:js:vendors:rethinkdb-websocket-client": "browserify -s RethinkdbWebsocketClient -r rethinkdb-websocket-client -o ./dist/www/lib/js/rethinkdb-websocket-client.js",
    "build:js:vendors": "npm-run-all --parallel build:js:vendors:*",
    "build:js:app:main": "rollup -n app -i ./src/www/main.js -o ./dist/www/js/main.js --config ./config/rollup.config_app.js",
    "build:js:app": "npm-run-all --parallel build:js:app:*",
    "build:js": "npm-run-all --parallel build:js:*",
    "build:css:chat": "cp ./src/css/chat.css ./dist/www/css/.",
    "build:css": "npm-run-all --parallel build:css:*",
    "html:rethinkdb": "babel-node ./src/server-render/rethinkdb.js",
    "html:anonchat": "babel-node ./src/server-render/anonchat.js",
    "build:html": "npm -s run html:anonchat > ./dist/www/index.html",
    "prebuild:webapp": "npm-run-all clean:www clean:css mkdir",
    "build:webapp": "npm-run-all --parallel build:html build:js build:css",
    "watch:js:app": "watch \"npm run build:js:app\" src/",
    "prepublish": "npm run build:npm",
    "gitpush": "git push origin master && git push calamar master",
    "ppp": "npm version patch && npm publish && npm run gitpush",
    "mpp": "npm version minor && npm publish && npm run gitpush"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/calamar-io/chat-logger.git"
  },
  "keywords": [
    "telegram",
    "logger",
    "bot",
    "reducer",
    "redux",
    "flux"
  ],
  "author": "Fabricio C Zuardi",
  "license": "AGPL-3.0",
  "bugs": {
    "url": "https://github.com/calamar-io/chat-logger/issues"
  },
  "homepage": "https://github.com/calamar-io/chat-logger#readme",
  "babel": {
    "presets": [
      "es2015",
      "stage-2",
      "stage-3",
      "react"
    ]
  },
  "devDependencies": {
    "amqplib": "^0.4.1",
    "ava": "^0.13.0",
    "babel-cli": "^6.6.5",
    "babel-core": "^6.7.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-es2015-rollup": "^1.1.1",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-2": "^6.5.0",
    "babel-preset-stage-3": "^6.5.0",
    "browserify": "^13.0.0",
    "eslint": "^2.5.3",
    "events": "^1.1.0",
    "js-beautify": "^1.6.2",
    "npm-run-all": "^1.6.0",
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-redux": "^4.4.1",
    "redux": "^3.3.1",
    "rethinkdb": "^2.2.3",
    "rethinkdb-websocket-client": "^0.4.6",
    "rethinkdb-websocket-server": "^0.5.0",
    "rollup": "^0.25.7",
    "rollup-plugin-babel": "^2.4.0",
    "rollup-plugin-commonjs": "^2.2.1",
    "rollup-plugin-node-resolve": "^1.5.0",
    "telegram-bot-api": "^1.0.0",
    "uuid": "^2.0.1",
    "wascally": "^0.2.10",
    "watch": "^0.17.1"
  },
  "dependencies": {
    "lodash": "^4.6.1"
  }
}
