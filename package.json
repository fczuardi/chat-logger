{
  "name": "chat-logger",
  "version": "0.6.3",
  "description": "A logger for Telegram messages",
  "main": "./dist/npm/index.js",
  "scripts": {
    "example:telegram": "babel-node ./examples/telegram.js",
    "example:amqplib": "babel-node ./examples/amq.js",
    "example:wascally": "babel-node ./examples/wascally.js",
    "start": "npm run example:telegram",
    "test": "ava --require 'babel-core/register'",
    "build:npm": "rollup --config",
    "prepublish": "npm run build:npm",
    "ppp": "npm version patch && npm publish && git push",
    "mpp": "npm version minor && npm publish && git push"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/calamar-io/chat-logger.git"
  },
  "keywords": [
    "telegram",
    "logger",
    "bot",
    "reducer",
    "redux",
    "flux"
  ],
  "author": "Fabricio C Zuardi",
  "license": "AGPL-3.0",
  "bugs": {
    "url": "https://github.com/calamar-io/chat-logger/issues"
  },
  "homepage": "https://github.com/calamar-io/chat-logger#readme",
  "babel": {
    "presets": [
      "es2015",
      "stage-2",
      "stage-3"
    ]
  },
  "devDependencies": {
    "amqplib": "^0.4.1",
    "ava": "^0.12.0",
    "babel-cli": "^6.6.5",
    "babel-core": "^6.7.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-es2015-rollup": "^1.1.1",
    "babel-preset-stage-2": "^6.5.0",
    "babel-preset-stage-3": "^6.5.0",
    "redux": "^3.3.1",
    "rollup": "^0.25.4",
    "rollup-plugin-babel": "^2.4.0",
    "rollup-plugin-multi-entry": "^1.1.0",
    "telegram-bot-api": "^1.0.0",
    "wascally": "^0.2.10"
  },
  "dependencies": {}
}
