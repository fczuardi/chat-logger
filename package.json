{
  "name": "chat-logger",
  "version": "0.7.4",
  "description": "A logger for Telegram messages",
  "main": "./dist/npm/index.js",
  "scripts": {
    "clean": "rm -rf ./dist",
    "mkdir": "mkdir -p ./dist/www/lib/js",
    "example:telegram": "babel-node ./examples/telegram.js",
    "example:amqplib": "babel-node ./examples/amq.js",
    "example:wascally": "babel-node ./examples/wascally.js",
    "start": "http-server ./dist/www",
    "test": "ava --require 'babel-core/register'",
    "build:npm:main": "rollup --config ./config/rollup.config_npm.js",
    "build:npm:debug": "rollup -i ./src/lib/debug.js -o ./dist/npm/debug.js --config ./config/rollup.config_cjs.js",
    "build:npm:helpers:amq": "rollup -i ./src/lib/amqHelpers.js -o ./dist/npm/amqHelpers.js --config ./config/rollup.config_cjs.js",
    "build:npm:helpers:wascally": "rollup -i ./src/lib/wascallyHelpers.js -o ./dist/npm/wascallyHelpers.js --config ./config/rollup.config_cjs.js",
    "build:npm:helpers:telegram": "rollup -i ./src/lib/telegramHelpers.js -o ./dist/npm/telegramHelpers.js --config ./config/rollup.config_cjs.js",
    "build:npm": "npm-run-all --parallel build:npm:**",
    "build:js:vendors:babelhelpers": "babel-external-helpers -t umd > ./dist/www/lib/js/babel-helpers.js",
    "build:js:app:reducer": "rollup -n loggerReducer -i ./src/lib/loggerReducer.js -o ./dist/www/js/chat-logger-reducer.js --config ./config/rollup.config_umd.js",
    "build:js:vendors": "npm-run-all --parallel build:js:vendors:*",
    "build:js:app": "npm-run-all --parallel build:js:app:*",
    "build:js": "npm-run-all --parallel build:js:*",
    "prebuild:html": "npm run mkdir",
    "debug:html": "babel-node ./src/bin/page.js",
    "build:html": "babel-node ./src/bin/page.js > ./dist/www/index.html",
    "watch:js": "watch \"npm run build:js:app\" src/",
    "prepublish": "npm run build:npm",
    "gitpush": "git push origin master && git push calamar master",
    "ppp": "npm version patch && npm publish && npm run gitpush",
    "mpp": "npm version minor && npm publish && npm run gitpush"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/calamar-io/chat-logger.git"
  },
  "keywords": [
    "telegram",
    "logger",
    "bot",
    "reducer",
    "redux",
    "flux"
  ],
  "author": "Fabricio C Zuardi",
  "license": "AGPL-3.0",
  "bugs": {
    "url": "https://github.com/calamar-io/chat-logger/issues"
  },
  "homepage": "https://github.com/calamar-io/chat-logger#readme",
  "babel": {
    "presets": [
      "es2015",
      "stage-2",
      "stage-3",
      "react"
    ]
  },
  "devDependencies": {
    "amqplib": "^0.4.1",
    "ava": "^0.12.0",
    "babel-cli": "^6.6.5",
    "babel-core": "^6.7.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-es2015-rollup": "^1.1.1",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-2": "^6.5.0",
    "babel-preset-stage-3": "^6.5.0",
    "js-beautify": "^1.6.2",
    "npm-run-all": "^1.6.0",
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-redux": "^4.4.1",
    "redux": "^3.3.1",
    "rollup": "^0.25.4",
    "rollup-plugin-babel": "^2.4.0",
    "rollup-plugin-multi-entry": "^1.1.0",
    "telegram-bot-api": "^1.0.0",
    "wascally": "^0.2.10",
    "watch": "^0.17.1"
  },
  "dependencies": {}
}
